{
    "version": 2,
    "builds": [
      {
        "src": "src/ndex.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
        "Access-Control-Allow-Origin": "*"
      }
      }
    ]
  }